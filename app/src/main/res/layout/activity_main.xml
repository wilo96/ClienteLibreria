<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/corrertxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:layout_centerHorizontal="true"
        android:layout_above="@id/textView2"/>

    <EditText
        android:id="@+id/contrase単atxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autofillHints=""
        android:password="true"
        android:ems="10"
        android:inputType="textPassword"
        android:text=""
        tools:ignore="MissingConstraints"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true" />

    <Button
        android:id="@+id/iniciobtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Iniciar Sesion"
        tools:ignore="MissingConstraints"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/contrase単atxt"
        />

    <Button
        android:id="@+id/registrobtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registrarse"
        tools:ignore="MissingConstraints"
        android:layout_below="@id/iniciobtn"
        android:layout_centerHorizontal="true"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Coreo Electronico:"
        tools:ignore="MissingConstraints"
        android:layout_centerHorizontal="true"
        android:layout_above="@id/corrertxt"
        />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contrase単a:"
        tools:ignore="MissingConstraints"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_above="@id/contrase単atxt"/>
</RelativeLayout>